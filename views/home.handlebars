<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="curiosity.jpg" class="c_img" alt="Curiosity" style="width:100%;">
	  <div class="carousel-caption">
		<h3>Curiosity</h3>
	  </div>
    </div>

    <div class="item">
      <img src="opportunity.jpg" class="c_img" alt="Opportunity" style="width:100%;">
      <div class="carousel-caption">
		<h3>Opportunity</h3>
	  </div>
	</div>

    <div class="item">
      <img src="spirit.jpg" class="c_img" alt="Spirit" style="width:100%;">
	  <div class="carousel-caption">
		<h3>Spirit</h3>
	  </div>
    </div>
  </div>

  <!-- Left and right controls -->
  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#myCarousel" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
<center>
<table id="rovers">
	<tr>
		<th>Rover</th>
		<th>Mission Status</th>
		<th>Launch Date</th>
		<th>Landing Date</th>
		<th>Martian Sol</th>
		<th>Earth Date</th>
		<th>Total No. of Photos</th>
	</tr>
	{{#each manifests}}
	<tr>
		<td>{{this.name}}</td>
		<td>{{this.status}}</td>
		<td>{{this.launch_date}}</td>
		<td>{{this.landing_date}}</td>
		<td>{{this.max_sol}}</td>
		<td>{{this.max_date}}</td>
		<td>{{this.total_photos}}</td>
	</tr>
	{{/each}}
</table>
</center>